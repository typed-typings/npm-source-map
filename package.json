{
  "name": "@types/source-map",
  "version": "0.5.0",
  "description": "Types for https://github.com/mozilla/source-map",
  "typings": "index.d.ts",
  "scripts": {
    "lint": "tslint \"**/*.ts\" -e \"node_modules/**\" -e \"typings/**\" -e bundle.d.ts",
    "bundle": "typings bundle -o bundle.d.ts",
    "exec": "tsc && ts-node test.ts",
    "test": "npm run lint && npm run bundle && npm run exec",
    "prepublish": "typings install",
    "postpublish": "greenkeeper-postpublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/types/npm-source-map.git"
  },
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/types/npm-source-map/issues"
  },
  "homepage": "https://github.com/types/npm-source-map#readme",
  "devDependencies": {
    "greenkeeper-postpublish": "^1.0.1",
    "source-map": "^0.5.6",
    "ts-node": "^3.3.0",
    "tslint": "^5.2.0",
    "tslint-config-typings": "^0.3.1",
    "typescript": "^2.0.3",
    "typings": "^2.0.0"
  }
}
